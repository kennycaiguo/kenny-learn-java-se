package cr.kenny.many2many;

import java.util.List;

//多方
public class Student {
   private Integer id;
   private String name;
   private List<Course> courseList;//关联属性
   
   public Student(){}

public Student(Integer id, String name, Course grade) {
	 
	this.id = id;
	this.name = name;
	this.grade = grade;
}

public Integer getId() {
	return id;
}

public void setId(Integer id) {
	this.id = id;
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

 

public Course getGrade() {
	return grade;
}

public void setGrade(Course grade) {
	this.grade = grade;
}

@Override
public String toString() {
	return "Student [id=" + id + ", name=" + name + ",grade=" + grade + "]";
}
   
}
