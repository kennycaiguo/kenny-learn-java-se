package cr.kenny.base;

public class Student {
	
	private Integer sid;
	private String sname;
	private Double ssal;
	
	
	public Student() {
		 
	}
	
	
	public Student(Integer sid, String sname, Double ssal) {
		 
		this.sid = sid;
		this.sname = sname;
		this.ssal = ssal;
	}


	public Integer getSid() {
		return sid;
	}
	public void setSid(Integer sid) {
		this.sid = sid;
	}
	public String getSname() {
		return sname;
	}
	public void setSname(String sname) {
		this.sname = sname;
	}
	public Double getSsal() {
		return ssal;
	}
	public void setSsal(Double ssal) {
		this.ssal = ssal;
	}


	@Override
	public String toString() {
		return "Student [sid=" + sid + ", sname=" + sname + ", ssal=" + ssal
				+ "]";
	}
	
	
}
